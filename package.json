{
  "name": "eleventy-starter-ghost",
  "version": "1.0.0",
  "description": "A starter template to build websites with Ghost & Eleventy",
  "main": ".eleventy.js",
  "scripts": {
    "start": "yarn run dev",
    "dev": "cross-env ELEVENTY_ENV=dev eleventy --serve",
    "build": "cross-env ELEVENTY_ENV=prod eleventy",
    "test": "yarn run build",
    "lint": "eslint . --ext .js --cache",
    "clean": "rimraf dist",
    "serve:webpack": "webpack --mode development --watch",
    "serve:eleventy": "ELEVENTY_ENV=development eleventy --serve",
    "serve": "npm-run-all clean --parallel serve:*",
    "build:webpack": "webpack --mode production",
    "build:eleventy": "ELEVENTY_ENV=production eleventy",
    "build:dep": "run-s clean build:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TryGhost/eleventy-starter-ghost.git"
  },
  "keywords": [
    "eleventy",
    "ghost",
    "starter",
    "boilerplate",
    "static",
    "site",
    "jamstack"
  ],
  "author": "David Darnes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TryGhost/eleventy-starter-ghost/issues"
  },
  "homepage": "https://github.com/TryGhost/eleventy-starter-ghost#readme",
  "dependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-plugin-rss": "^1.1.1",
    "@tryghost/content-api": "^1.4.4",
    "@tryghost/helpers": "^1.1.6",
    "clean-css": "^4.2.1",
    "cross-env": "^7.0.0",
    "dotenv": "^8.0.0",
    "eleventy-plugin-lazyimages": "https://github.com/KomodoPlatform/eleventy-plugin-lazyimages#d8fe756a3880d0832044a49a3176644886c29dc7",
    "eleventy-plugin-local-images": "^0.4.0",
    "eslint-utils": "3.0.0",
    "html-minifier": "^4.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.6",
    "css-loader": "^5.2.6",
    "eslint": "7.32.0",
    "eslint-plugin-ghost": "2.7.0",
    "mini-css-extract-plugin": "^1.6.0",
    "postcss-loader": "^5.3.0",
    "rimraf": "^3.0.2",
    "tailwindcss": "^2.1.4",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.2",
    "yarn-run-all": "^3.1.1"
  }
}
