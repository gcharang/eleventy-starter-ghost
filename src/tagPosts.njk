---
pagination:
  data: tagPostsCollection
  size: 1
  alias: postSet
permalink: "tags/{{ postSet.url }}"
---

{% extends 'layouts/default.njk' %}
{% from "partials/card.njk" import card %}

{% set title = "tagss" %}

{% block content %}
    <section class="post-feed">
        {% for post in postSet.posts %}
            {{ card(post) }}
        {% endfor %}
    </section>

    {% include "partials/pagination.njk" %}
{% endblock%}